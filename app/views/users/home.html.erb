

<div id="myCarousel" class="carousel slide" data-ride="carousel">
      <!-- Indicators -->
      <ol class="carousel-indicators">
        <li data-target="#myCarousel" data-slide-to="0" class="active"></li>
        <li data-target="#myCarousel" data-slide-to="1"></li>
        <li data-target="#myCarousel" data-slide-to="2"></li>
      </ol>
      <div class="carousel-inner" role="listbox">
        <div class="item active">
         
          <img class="first-slide" <%= link_to image_tag("slideone.svg", alt: "First slide") %>
        
          <div class="countainer">
          <div class="carousel-caption">
              <h1>Welcome to CLOSE To Home, a tool that helps you pinpoint valuable resources in your community</h1>
            </div>
          </div>
        </div>
        <div class="item">
          <img class="second-slide"  <%= link_to image_tag("slidetwo.svg", alt: "Second slide") %>>
          <div class="container">
            <div class="carousel-caption">
              <h1>Just enter an address and the school ranges for your children to find public schools, parks and programs in your neighborhood.</h1>
            </div>
          </div>
        </div>
        <div class="item">
          <img class="third-slide" <%= link_to image_tag("slidethree.svg", alt: "Third slide") %>>
          <div class="container">
            <div class="carousel-caption">
              <h1>Whether you are relocating to a new neighborhood or becoming a first-time parent, Close To Home will help you track down great resources for your family in your area.</h1>
            </div>
          </div>
        </div>
      </div>
      <a class="left carousel-control" href="#myCarousel" role="button" data-slide="prev">
        <span class="glyphicon glyphicon-chevron-left" aria-hidden="true"></span>
        <span class="sr-only"> P </span>
      </a>
      <a class="right carousel-control" href="#myCarousel" role="button" data-slide="next">
        <span class="glyphicon glyphicon-chevron-right" aria-hidden="true"></span>
        <span class="sr-only"> N </span>
      </a>
    </div><!-- /.carousel -->
    <div>
    <div class="posRowMarginTop posRowMarginBottom">
    <div class="row">
      <div class="col-xs-12 posCenter">
     <%= link_to "Enter", users_search_path, class: "btn btn-danger btn-lg" %>
     </div>
    </div> 
    </div>
</div>

